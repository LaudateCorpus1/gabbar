{
  "name": "gabbar",
  "version": "0.2.4",
  "description": "Guarding OpenStreetMap from invalid or suspicious edits!",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint index.js **/*.js --ignore-pattern '!node_modules/*'",
