    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "naughty-words": "^1.0.1",
    "real-changesets-parser": "^1.2.3",
    "request": "^2.81.0",
    "simple-statistics": "^4.1.0",
